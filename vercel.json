{
    "headers": [
        {
            "source": "/static/(.*)\\.(jpg|jpeg|gif|png|svg|ico|webp)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=86400, must-revalidate"
                }
            ]
        },
        {
            "source": "/static/(.*)\\.(js|css)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}